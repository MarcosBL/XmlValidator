{
  "name": "seromenho/xml-validator",
  "description": "Validates your XML against a xsd schema file.",
  "keywords": ["XML", "XSD", "XML Validator", "psr-4"],
  "homepage": "http://github.com/seromenho/XmlValidator",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Seromenho",
      "email": "ricardo@seromenho.com",
      "homepage": "http://seromenho.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*"
  },
  "autoload": {
    "psr-4": {"XmlValidator\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"XmlValidator\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}